// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                              String             @id @default(cuid())
  fullName                        String             @default("")
  email                           String             @unique @default("")
  password                        String
  phone                           String             @default("")
  address                         String             @default("")
  createdAt                       DateTime?          @default(now())
  updatedAt                       DateTime?          @default(now())
  from_Invoice_user               Invoice[]          @relation("Invoice_user")
  from_responsiveConfig_updatedBy responsiveConfig[] @relation("responsiveConfig_updatedBy")
}

model Agency {
  id                                String             @id @default(cuid())
  agencyFullName                    String             @default("")
  agencyAvatar_id                   String?
  agencyAvatar_filesize             Int?
  agencyAvatar_width                Int?
  agencyAvatar_height               Int?
  agencyAvatar_extension            String?
  agencyPhoneNumber                 String             @default("")
  agencyAddress                     String             @default("")
  agencyTaxNumber                   String             @default("")
  agencyCompanyType                 String?
  cars                              Car[]              @relation("Agency_cars")
  createdAt                         DateTime?          @default(now())
  updatedAt                         DateTime?          @default(now())
  from_Car_agency                   Car[]              @relation("Car_agency")
  from_Invoice_agency               Invoice[]          @relation("Invoice_agency")
  from_carSpecification_agency      carSpecification[] @relation("carSpecification_agency")
  from_contactInquiry_relatedAgency contactInquiry[]   @relation("contactInquiry_relatedAgency")
}

model Car {
  id                         String              @id @default(cuid())
  name                       String              @default("")
  model                      String
  year                       Int
  price                      Int
  color                      String?
  bodyType                   String?
  engineType                 String?
  horsepower                 Int?
  torque                     Int?
  topSpeed                   Int?
  acceleration               String              @default("")
  transmission               String?
  drivetrain                 String?
  fuelCapacity               Float?
  fuelEconomyCity            Float?
  fuelEconomyHighway         Float?
  fuelEconomyCombined        Float?
  emissionsRating            String              @default("")
  airbags                    Int?
  abs                        Boolean             @default(false)
  tractionControl            Boolean             @default(false)
  laneAssist                 Boolean             @default(false)
  adaptiveCruiseControl      Boolean             @default(false)
  blindSpotMonitor           Boolean             @default(false)
  rearViewCamera             Boolean             @default(false)
  parkingSensors             Boolean             @default(false)
  autonomousDrivingLevel     String?
  seats                      Int?
  seatMaterial               String?
  heatedSeats                Boolean             @default(false)
  ventilatedSeats            Boolean             @default(false)
  sunroof                    Boolean             @default(false)
  climateControl             String?
  infotainmentSystem         String              @default("")
  touchscreenSize            Float?
  wirelessCharging           Boolean             @default(false)
  appleCarPlay               Boolean             @default(false)
  androidAuto                Boolean             @default(false)
  wheels                     String?
  wheelSize                  Float?
  ledHeadlights              Boolean             @default(false)
  fogLights                  Boolean             @default(false)
  tintedWindows              Boolean             @default(false)
  cargoCapacity              Float?
  towingCapacity             Float?
  roofRack                   Boolean             @default(false)
  trailerHitch               Boolean             @default(false)
  bluetooth                  Boolean             @default(false)
  wifiHotspot                Boolean             @default(false)
  usbPorts                   Int?
  speakerSystem              String              @default("")
  agency                     Agency[]            @relation("Car_agency")
  status                     String?             @default("available")
  listedDate                 DateTime?           @default(now())
  warrantyYears              Int?
  images_id                  String?
  images_filesize            Int?
  images_width               Int?
  images_height              Int?
  images_extension           String?
  videoURL                   String              @default("")
  insuranceStatus            String?
  registrationValidUntil     DateTime?
  from_Agency_cars           Agency[]            @relation("Agency_cars")
  from_Invoice_car           Invoice[]           @relation("Invoice_car")
  from_reviewsAndRatings_car reviewsAndRatings[] @relation("reviewsAndRatings_car")
  from_carSpecification_car  carSpecification[]  @relation("carSpecification_car")
}

model Invoice {
  id           String   @id @default(cuid())
  orderId      String   @default("")
  taxOnValue   String   @default("")
  BaseTaxValue String   @default("")
  NetValue     String   @default("")
  status       Boolean  @default(false)
  agency       Agency[] @relation("Invoice_agency")
  user         User[]   @relation("Invoice_user")
  car          Car[]    @relation("Invoice_car")
}

model settings {
  id              String  @id @default(cuid())
  brand           String
  bodyType        String?
  engineType      String?
  transmission    String?
  drivetrain      String?
  climateControl  String?
  wheels          String?
  heatedSeats     Boolean @default(false)
  ventilatedSeats Boolean @default(false)
  sunroof         Boolean @default(false)
  bluetooth       Boolean @default(false)
  wifiHotspot     Boolean @default(false)
  appleCarPlay    Boolean @default(false)
  androidAuto     Boolean @default(false)
  airbags         Int?
  warrantyYears   Int?
  fuelCapacity    Float?
}

model responsiveConfig {
  id                String    @id @default(cuid())
  name              String    @unique @default("")
  isMobileOptimized Boolean   @default(true)
  breakpoints       String?
  designFeatures    String?
  theme             String?   @default("light")
  createdAt         DateTime? @default(now())
  updatedAt         DateTime? @default(now())
  updatedBy         User?     @relation("responsiveConfig_updatedBy", fields: [updatedById], references: [id])
  updatedById       String?   @map("updatedBy")

  @@index([updatedById])
}

model reviewsAndRatings {
  id            String    @id @default(cuid())
  reviewTitle   String    @default("")
  reviewText    String    @default("")
  rating        Int
  car           Car?      @relation("reviewsAndRatings_car", fields: [carId], references: [id])
  carId         String?   @map("car")
  reviewerName  String    @default("")
  reviewerEmail String    @default("")
  createdAt     DateTime? @default(now())
  updatedAt     DateTime? @default(now())

  @@index([carId])
}

model media {
  id                  String    @id @default(cuid())
  title               String    @default("")
  description         String    @default("")
  mediaType           String
  mediaFile_id        String?
  mediaFile_filesize  Int?
  mediaFile_width     Int?
  mediaFile_height    Int?
  mediaFile_extension String?
  videoURL            String    @default("")
  createdAt           DateTime? @default(now())
  updatedAt           DateTime? @default(now())
}

model carSpecification {
  id                 String    @id @default(cuid())
  car                Car?      @relation("carSpecification_car", fields: [carId], references: [id])
  carId              String?   @map("car")
  agency             Agency?   @relation("carSpecification_agency", fields: [agencyId], references: [id])
  agencyId           String?   @map("agency")
  engineType         String?
  horsepower         Int?
  torque             Int?
  topSpeed           Int?
  acceleration       String    @default("")
  transmission       String?
  drivetrain         String?
  fuelCapacity       Float?
  fuelEconomyCity    Float?
  fuelEconomyHighway Float?
  emissionsRating    String    @default("")
  airbags            Int?
  warrantyYears      Int?
  images_id          String?
  images_filesize    Int?
  images_width       Int?
  images_height      Int?
  images_extension   String?
  listedDate         DateTime? @default(now())

  @@index([carId])
  @@index([agencyId])
}

model contactInquiry {
  id              String    @id @default(cuid())
  fullName        String    @default("")
  phoneNumber     String    @default("")
  message         String    @default("")
  inquiryType     String?   @default("general")
  relatedAgency   Agency?   @relation("contactInquiry_relatedAgency", fields: [relatedAgencyId], references: [id])
  relatedAgencyId String?   @map("relatedAgency")
  createdAt       DateTime? @default(now())
  updatedAt       DateTime? @default(now())

  @@index([relatedAgencyId])
}
